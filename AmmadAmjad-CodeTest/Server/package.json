{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for Hacker News Project for WiredCraft code challenge.",
  "main": "backend.js",
  "scripts": {
    "start": "node backend.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
     "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "keywords": [
    "wiredcreaft",
    "code",
    "challenge",
    "hacker",
    "news"
  ],
  "author": "Ammad Amjad Tarar",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.12",
    "sqlite3": "^4.2.0",
    "underscore": "^1.10.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jest": "^26.0.1",
    "supertest": "^4.0.2"
  }
}

    "build": "webpack build"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.6",
    "aws-amplify": "^4.3.27",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"

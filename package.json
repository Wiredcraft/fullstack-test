  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@imaginary-cloud/eslint-config-react": "^2.0.1",
    "@imaginary-cloud/prettier-config": "^1.0.0",
